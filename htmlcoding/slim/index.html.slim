doctype html
html
	head
		meta charset="utf-8"
		meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, minimum-scale=1, maximum-scale=1"
		title Example
		link rel="stylesheet" href="assets/css/reset.css" type="text/css"
		link rel="stylesheet" href="assets/css/screen.css" type="text/css"
		link rel="stylesheet" href="assets/css/transition.css" type="text/css"

	body
		div#wrapper
			div#underLeft
			div#underRight
			div#container.panel
				div#main.content
					header
						h1 
							img src="assets/images/logo.png" width="235" height="63"

					nav
						| {% for entry in entries %}
						li
							a href="{{entry.id}}"
								img src="assets/images/menu.png" width="104" height="29"
						| {% endfor %}
						
					footer
						img src="assets/images/copyright.png" width="320" height="14"
			div#reverse.panel
				
		script type="text/javascript" src="assets/js/libs/head.load.min.js"
		script type="text/javascript" src="assets/js/main.js"